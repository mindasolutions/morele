{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
      "php": ">=8.3",
      "ext-mbstring": "*"
    },
    "config": {
        "allow-plugins": {
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit --configuration phpunit.xml.dist"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
